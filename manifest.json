{
  "manifest_version": 3,
  "name": "Smiling dog",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "dijk_smiling.jpeg"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["faceapi.min.js", "contentScript.js"]
  }]
}
